//_____________________________________ * Source code to find factorial of a number. ________________________



#include<stdio.h>

int factorial (int n);
int main() {
	int n,a;

printf("Enter an positive integer:" );

scanf ( "%d ",&n);

a=n, factorial (n)
printf("Factorial of %d=%ld ",a ) ;
return 0;
}

int factorial (int n){
int a;
if(n!=1){
a=n*factorial(n-1);
	return a;
}
}

