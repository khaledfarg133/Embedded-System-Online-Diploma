//_____________________________________ * Source code to find factorial of a number. ________________________



#include<stdio.h>

int factorial (int n);
int main() {
	int n;

printf("Enter an positive integer:" );

scanf ( "%d ",&n);

printf("Factoria1 of %d=%ld ",n, factorial (n) ) ;
return 0;
}

int factorial (int n){

if(n!=l)

	return n*factorial(n-l);
}

